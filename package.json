{
  "name": "LEGION",
  "version": "0.1.0",
  "description": "A revolution solution for design research",
  "private": true,
  "scripts": {
    "start": "cross-env BROWSER=none ESLINT=none umi dev",
    "start:no-proxy": "cross-env NO_PROXY=true ESLINT=none umi dev",
    "build": "cross-env ESLINT=none umi build",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "cross-env ANALYZE=true umi build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js --fix",
    "precommit": "npm run lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "test": "umi test",
    "test:storybook": "jest --json --outputFile=.jest-result-for-storybook.json || true",
    "test:component": "umi test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "storybook": "start-storybook -p 9001 -c storybook/config",
    "storybook:test": "npm run test:storybook && start-storybook -p 9001 -c storybook/config"
  },
  "dependencies": {
    "@antv/data-set": "^0.8.5",
    "@babel/polyfill": "^7.0.0-beta.40",
    "antd": "^3.3.3",
    "babel-runtime": "^6.26.0",
    "bizcharts": "^3.1.5",
    "bizcharts-plugin-slider": "^2.0.3",
    "classnames": "^2.2.5",
    "dva": "^2.2.2",
    "dva-loading": "^1.0.4",
    "enquire-js": "^0.1.2",
    "fastclick": "^1.0.6",
    "lodash": "^4.17.5",
    "lodash-decorators": "^4.5.0",
    "moment": "^2.21.0",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.2.0",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "rc-drawer-menu": "^0.5.7",
    "react-beautiful-dnd": "^6.0.2",
    "react-container-query": "^0.9.1",
    "react-document-title": "^2.0.3",
    "react-fittext": "^1.0.0",
    "react-hot-keys": "^1.0.11",
    "react-hot-loader": "^4.0.1",
    "react-reflex": "^2.2.3",
    "styled-components": "^3.2.2",
    "umi": "^1.1.5",
    "umi-plugin-dva": "^0.6.1",
    "url-polyfill": "^1.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.0",
    "@storybook/addon-a11y": "^3.4.1",
    "@storybook/addon-actions": "^3.4.1",
    "@storybook/addon-centered": "^3.4.1",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.4.1",
    "@storybook/addon-jest": "^3.4.1",
    "@storybook/react": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.7",
    "eslint-plugin-react": "^7.0.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "less-vars-to-js": "^1.2.1",
    "lint-staged": "^6.0.0",
    "mockjs": "^1.0.1-beta3",
    "pro-download": "^1.0.1",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.11.1",
    "roadhog": "^2.1.0",
    "roadhog-api-doc": "^0.3.4",
    "rollbar": "^2.3.4",
    "storybook-addon-jsx": "^5.3.0",
    "storybook-host": "^4.1.5",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "prettier --trailing-comma all --single-quote --write",
      "lint-staged:js"
    ],
    "**/*.less": "stylelint --syntax less --fix"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}

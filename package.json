{
  "name": "Lingmao",
  "version": "0.1.0",
  "description": "A revolution solution for design research",
  "private": true,
  "scripts": {
    "start": "cross-env NO_PROXY=true COMPILE_ON_DEMAND=none BROWSER=none HARD_SOURCE=none  ESLINT=none TS_CONFIG_PATHS_PLUGIN=none umi dev",
    "start:proxy": "cross-env BROWSER=none ESLINT=none umi dev",
    "build": "cross-env ESLINT=none umi build",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "cross-env ANALYZE=true umi build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "precommit": "npm run lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "test": "umi test",
    "test:watch": "umi test --watch",
    "cov": "umi test --coverage",
    "test:all": "node ./tests/run-tests.js"
  },
  "dependencies": {

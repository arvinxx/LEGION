    "umi-plugin-dva": "^0.7.2",
    "umi-plugin-polyfill": "^0.1.0",
    "umi-plugin-routes": "^0.1.2"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "prettier --trailing-comma all --single-quote --write"
    ],
    "**/*.less": "stylelint --syntax less --fix"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}

    "react-document-title": "^2.0.3",
    "react-fittext": "^1.0.0",
    "react-hot-loader": "^4.2.0",
    "react-hotkeys": "^1.1.4",
    "react-reflex": "^2.2.3",
    "shortid": "^2.2.8",
    "slate": "^0.33.6",
    "slate-plain-serializer": "^0.5.12",
    "slate-react": "^0.12.6",
    "styled-components": "^3.2.2",
    "url-polyfill": "^1.0.12",
    "xlsx": "^0.12.12"

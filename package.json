    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.8",
    "enquire-js": "^0.1.2",
    "fastclick": "^1.0.6",
    "highlightable": "^1.0.7",
    "immutability-helper": "^2.7.0",
    "lodash": "^4.17.5",
